{
    "name": "ellis/oxid-console",
    "description": "OXID Console is php console application for OXID Shop. It provides an API for writing various commands. We have shipped very common for every oxid shop project commands out of the box.",
    "type": "library",
    "keywords": ["oxid", "console", "command"],
    "homepage": "https://github.com/EllisV/oxid-console",
    "license": "MIT",
    "authors": [
        {
            "name": "Eligijus Vitkauskas",
            "email": "eligijusvitkauskas@gmail.com",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/EllisV/oxid-console/issues"
    },
    "require": {
        "symfony/console": "^2.5 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": { "Ellis\\Oxid\\Console\\": "src/" },
        "classmap": ["backports/"]
    },
    "bin": ["bin/oxid"]
}
